	folder "Header Files"
	{
		"ModelCheckIn.h"
		"ModelCheckInDlg.h"
		"Resource.h"
		"StdAfx.h"
	}

	folder "Resources"
	{
		"res/ModelCheckIn.ico"
		"res/ModelCheckIn.rc2"
	}
}
