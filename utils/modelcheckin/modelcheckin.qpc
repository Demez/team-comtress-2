macro PROJECT_NAME "ModelCheckIn"

macro SRCDIR "../.."
macro OUTBINDIR "$SRCDIR$/../game/bin"

include "$SRCDIR$/_qpc_scripts/source_exe_win_win32_base.qpc"

config
{
	compile
	{
		pch "use"
		pch_out "Debug/modelcheckin.pch"
	}
}

files
