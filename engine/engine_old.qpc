macro PROJECT_NAME "engine"

macro SRCDIR ".."
macro OUTBINDIR "$SRCDIR$/../game/bin"

include "$SRCDIR$/_qpc_scripts/source_dll_base.qpc"
include "$SRCDIR$/_qpc_scripts/source_replay.qpc"
include "$SRCDIR$/_qpc_scripts/dxmath_include.qpc"

macro GPROFILER "1" [$LINUX$ && $STAGING_ONLY$]

config
{
	compile
	{
		defines
		{
			"DEDICATED" "SWDS" [$DEDICATED$]
			"USE_WEBM_FOR_REPLAY" [$LINUX$]
			"GPROFILER" [$GPROFILER$]
			"ENABLE_RPT" [$ENABLE_RPT$]
			"USE_CONVARS"
			"VOICE_OVER_IP"
			"BUMPMAP"
			"__USEA3D"
			"_ADD_EAX_"
			"ENGINE_DLL"
			"VERSION_SAFE_STEAM_API_INTERFACES"
			"USE_BREAKPAD_HANDLER"
			"NEW_DXSDK"
		}
		
		inc_dirs
		{
			"$SRCDIR$/dx9sdk/include" [$WINDOWS$]
			"../common/quicktime_win32/" [$QUICKTIME_WIN32$]
			"$SRCDIR$/thirdparty/gperftools-2.0/src/" [$GPROFILER$]
