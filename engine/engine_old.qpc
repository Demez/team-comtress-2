			"xonline.lib" "xaudio2.lib" "Xhv2.lib" "xmp.lib" "xmcore.lib" "xmahal.lib" "xparty.lib" [$X360$ && $RELEASE$]
			"socketlib" "$SRCDIR$/DX9SDK/lib/dsound" "$SRCDIR$/DX9SDK/lib/dxguid" [$WIN32$]
			"$LIBCOMMON$/libjpeg" "matsys_controls" "vgui_controls" [!$DEDICATED$]
			"$LIBCOMMON$/curl" [$MACOS$]
			"$LIBCOMMON$/$(CRYPTOPPDIR)/libcrypto" [$LINUX$ && !$DEDICATED$]
			"$SRCDIR$/thirdparty/gperftools-2.0/.libs/libprofiler" [$GPROFILER$]
			"$LIBCOMMON$/binkw32" [$WIN32$ && !$QUICKTIME_WIN32$]
