			"audio"
			"audio/private"
			"audio/public"
			"$SRCDIR$/vgui2/include"
			"$SRCDIR$/vgui2/controls"
		}
		
		options
		{
			"/arch:SSE2" [$WIN32$]
		}
	}
	
	link
	{
		libs
		{
			"dinput8.lib" "winmm.lib" "wsock32.lib" "ws2_32.lib" "wininet.lib" "vfw32.lib" "Rpcrt4.lib" [$WINDOWS$]
